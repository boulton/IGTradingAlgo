{
  "$id": "http://example.com/example.json",
  "$schema": "http://json-schema.org/draft-06/schema#",
  "definitions": {},
  "properties": {
    "instrumentType": {
      "$id": "/properties/instrumentType",
      "type": "string",
      "title": "The Instrumenttype Schema",
      "description": "An explanation about the purpose of this instance.",
      "default": "",
      "examples": [
        "CURRENCIES"
      ]
    },
    "metadata": {
      "$id": "/properties/metadata",
      "type": "object",
      "properties": {
        "allowance": {
          "$id": "/properties/metadata/properties/allowance",
          "type": "object",
          "properties": {
            "remainingAllowance": {
              "$id": "/properties/metadata/properties/allowance/properties/remainingAllowance",
              "type": "integer",
              "title": "The Remainingallowance Schema",
              "description": "An explanation about the purpose of this instance.",
              "default": 0,
              "examples": [
                9845
              ]
            },
            "totalAllowance": {
              "$id": "/properties/metadata/properties/allowance/properties/totalAllowance",
              "type": "integer",
              "title": "The Totalallowance Schema",
              "description": "An explanation about the purpose of this instance.",
              "default": 0,
              "examples": [
                10000
              ]
            },
            "allowanceExpiry": {
              "$id": "/properties/metadata/properties/allowance/properties/allowanceExpiry",
              "type": "integer",
              "title": "The Allowanceexpiry Schema",
              "description": "An explanation about the purpose of this instance.",
              "default": 0,
              "examples": [
                595011
              ]
            }
          }
        },
        "size": {
          "$id": "/properties/metadata/properties/size",
          "type": "integer",
          "title": "The Size Schema",
          "description": "An explanation about the purpose of this instance.",
          "default": 0,
          "examples": [
            25
          ]
        },
        "pageData": {
          "$id": "/properties/metadata/properties/pageData",
          "type": "object",
          "properties": {
            "pageSize": {
              "$id": "/properties/metadata/properties/pageData/properties/pageSize",
              "type": "integer",
              "title": "The Pagesize Schema",
              "description": "An explanation about the purpose of this instance.",
              "default": 0,
              "examples": [
                25
              ]
            },
            "pageNumber": {
              "$id": "/properties/metadata/properties/pageData/properties/pageNumber",
              "type": "integer",
              "title": "The Pagenumber Schema",
              "description": "An explanation about the purpose of this instance.",
              "default": 0,
              "examples": [
                1
              ]
            },
            "totalPages": {
              "$id": "/properties/metadata/properties/pageData/properties/totalPages",
              "type": "integer",
              "title": "The Totalpages Schema",
              "description": "An explanation about the purpose of this instance.",
              "default": 0,
              "examples": [
                1
              ]
            }
          }
        }
      }
    },
    "prices": {
      "$id": "/properties/prices",
      "type": "array",
      "mergeStrategy": "ArrayMergeById",
      "items": {
        "$id": "/properties/prices/items",
        "type": "object",
        "properties": {
          "snapshotTime": {
            "$id": "/properties/prices/items/properties/snapshotTime",
            "type": "string",
            "title": "The Snapshottime Schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
              "2017/11/22 00:00:00"
            ]
          },
          "snapshotTimeUTC": {
            "$id": "/properties/prices/items/properties/snapshotTimeUTC",
            "type": "string",
            "title": "The Snapshottimeutc Schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
              "2017-11-21T23:00:00"
            ]
          },
          "closePrice": {
            "$id": "/properties/prices/items/properties/closePrice",
            "type": "object",
            "properties": {
              "bid": {
                "$id": "/properties/prices/items/properties/closePrice/properties/bid",
                "type": "number",
                "title": "The Bid Schema",
                "description": "An explanation about the purpose of this instance.",
                "default": 0,
                "examples": [
                  8.426609992980957
                ]
              },
              "ask": {
                "$id": "/properties/prices/items/properties/closePrice/properties/ask",
                "type": "number",
                "title": "The Ask Schema",
                "description": "An explanation about the purpose of this instance.",
                "default": 0,
                "examples": [
                  8.431610107421875
                ]
              },
              "lastTraded": {
                "$id": "/properties/prices/items/properties/closePrice/properties/lastTraded",
                "type": "null",
                "title": "The Lasttraded Schema",
                "description": "An explanation about the purpose of this instance.",
                "default": null,
                "examples": [
                  null
                ]
              }
            }
          }
        }
      }
    }
  },
  "type": "object"
}